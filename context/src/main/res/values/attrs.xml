<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlowLinearLayout">
        <attr name="gravity" format="enum">
            <enum name="right" value="0" />
            <enum name="left" value="1" />
            <enum name="center" value="2" />
        </attr>
        <attr name="horizontal_Space" format="dimension" />
        <attr name="vertical_Space" format="dimension" />
        <attr name="isFull" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LoadingView">
        <attr name="loading_view_size" format="dimension" />
        <attr name="android:color" />
    </declare-styleable>

    <attr name="LoadingStyle" format="reference" />

    <declare-styleable name="BaseDialogLinearLayout">
        <attr name="android:background" />
    </declare-styleable>

</resources>